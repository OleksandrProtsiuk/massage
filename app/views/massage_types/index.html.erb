<p id="notice"><%= notice %></p>

<h1>Massage Types</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @massage_types.each do |massage_type| %>
      <tr>
        <td><%= massage_type.title %></td>
        <td><%= massage_type.body %></td>
        <td><%= link_to 'Show', massage_type %></td>
        <td><%= link_to 'Edit', edit_massage_type_path(massage_type) %></td>
        <td><%= link_to 'Destroy', massage_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Massage Type', new_massage_type_path %>
